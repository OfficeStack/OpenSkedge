{% extends 'OpenSkedgeBundle:Dashboard:index.html.twig' %}
{% block title %}{{ app_brand_name() }} - Error - {{ error.title }}{% endblock %}
{% block modulecontent %}
<div class="span12">
    <h3>{{ action }}</h3>
    <hr>
    <div class="alert alert-error">
        <strong>Error!</strong> {{ error.msg }}
    </div>
</div>
{% endblock %}
